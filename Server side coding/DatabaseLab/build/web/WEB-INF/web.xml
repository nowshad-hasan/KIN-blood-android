        <url-pattern>/AvailableDonorServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UnavailableDonorServlet</servlet-name>
        <url-pattern>/UnavailableDonorServlet</url-pattern>
